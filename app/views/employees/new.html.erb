<h1>Create A New Employee</h1>

<%= form_for @employee do | f | %>

    First Name: <%= f.text_field :first_name %><br><br>
    Last Name: <%= f.text_field :last_name %><br><br>
    Alias: <%= f.text_field :alias %><br><br>
    Title: <%= f.text_field :title %><br><br>
    Office: <%= f.text_field :office %><br><br>
    Image URL Address: <%= f.text_field :img_url %><br><br>
    Dogs: <%= f.collection_select :dog_id, @dogs, :id, :name %><br><br>

    <%= f.submit %>

<% end %>